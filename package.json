{
  "name": "react-starter",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- -w",
    "live-server": "live-server --wait=500 --port=3004 --ignore=node_modules,src --open=/dist/",
