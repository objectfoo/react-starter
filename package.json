{
  "name": "react-starter",
  "private": true,
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp build",
    "watch": "gulp",
    "live-server": "live-server --port=3004 --watch=./dist --open=/dist/",
    "test": "ava --verbose --harmony",
    "test:watch": "npm run test -- --watch"
  },
  "ava": {
    "tap": true,
    "failFast": true,
    "files": [
      "./src/**/__tests__/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "./jsdom_setup.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.10.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "cross-env": "^2.0.0",
    "debug": "^2.2.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.5.0",
    "eslint-plugin-ava": "^3.0.0",
    "eslint-plugin-react": "^6.2.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jsdom": "^9.5.0",
    "live-server": "^1.0.0",
    "minifyify": "^7.3.3",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {}
}
