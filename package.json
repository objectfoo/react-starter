    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "live-server": "^1.0.0",
    "minifyify": "^7.3.3",
    "mocha": "^2.5.3",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
